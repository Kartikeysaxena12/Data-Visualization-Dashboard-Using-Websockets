{
  "builds": [
    {
      "src": "admin-dashboard/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/socket",
      "dest": "backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "admin-dashboard/$1"
    }
  ]
}
